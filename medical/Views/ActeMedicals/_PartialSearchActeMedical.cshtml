@model IEnumerable<medical.Model.Models.ActeMedical>


@if (Model.Count() == 0)
{
    <div class="alert alert-info">Aucun Acte Medical trouvé</div>
}
<div style="max-height:300px;overflow-y:auto;" id="grouplistcontainer">
    @foreach (var item in Model)
    {
        <div>
            <div>@Html.ActionLink(item.ACTE, "Index", "ActeMedicals", new { id = item.IDACTEMEDICAUX }, null)</div>
            <div><small>Du Patient:  @item.Patient.NomComplet.ToString() </small></div>
            <div><small>Par :  @item.Medecin.NomComplet.ToString() </small></div>
        </div>
        <hr style="margin:23px 0px 0px 0px;" />
    }
</div>

