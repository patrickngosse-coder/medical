@model IEnumerable<medical.Model.Models.ActeInfiemier>


@if (Model.Count() == 0)
{
    <div class="alert alert-info">Aucun Acte Infirmier trouvé</div>
}
<div style="max-height:300px;overflow-y:auto;" id="grouplistcontainer">
    @foreach (var item in Model)
    {
        <div>
            <div>@Html.ActionLink(item.ACTE, "Index", "ActeInfiemiers", new { id = item.IDACTEINFIRMIER }, null)</div>
            <div><small>Du Patient:  @item.Patient.NomComplet.ToString() </small></div>
            <div><small>Par :  @item.Infirmier.NomComplet.ToString() </small></div>
        </div>
        <hr style="margin:23px 0px 0px 0px;" />
    }
</div>
