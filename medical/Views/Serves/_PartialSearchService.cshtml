@model IEnumerable<medical.Model.Models.Serve>


@if (Model.Count() == 0)
{
    <div class="alert alert-info">Aucun Patient trouvé</div>
}
<div style="max-height:300px;overflow-y:auto;" id="grouplistcontainer">
    @foreach (var item in Model)
    {
        <div>
            <div>@Html.ActionLink(item.DESIGNATION, "Index", "Patients", new { id = item.IDSERVICE }, null)</div>
        </div>
        <hr style="margin:23px 0px 0px 0px;" />
    }
</div>

